{
    "manifest_version": 2,
    "name": "Instagram Post Extension",
    "version": "1.0",
    "description": "Instagram Post Extension",
    "permissions": [
      "activeTab",
      "https://igapi.codermert1.repl.co/*"
    ],
    "browser_action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "images/icon16.png",
        "48": "images/icon16.png",
        "128": "images/icon16.png"
      }
    },
    "icons": {
      "16": "images/icon16.png",
      "48": "images/icon16.png",
      "128": "images/icon16.png"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ]
  }
  